{
  "name": "eslint-config-udyux",
  "version": "1.2.3",
  "description": "An opinionated and portable eslint config.",
  "author": "Nicolas Udy <dev@udy.io>",
  "main": "index.js",
  "files": [
    "vue.js",
    "prettier.js",
    "rules/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/udyux/eslint-config-udyux.git"
